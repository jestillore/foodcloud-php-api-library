{
    "name": "foodcloud/api-library",
    "description": "A PHP library to interface with the FoodCloud API.",
    "keywords": [
        "foodcloud",
        "api",
        "library"
    ],
    "homepage": "https://github.com/jestillore/foodcloud-php-api-library.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jillberth Estillore",
            "email": "ejillberth@gmail.com",
            "homepage": "http://ejillberth.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Foodcloud\\APILibrary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foodcloud\\APILibrary\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
